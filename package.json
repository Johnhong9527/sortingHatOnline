    "gh-pages": "^6.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vue-tsc": "^3.2.2"
  },
  "volta": {
